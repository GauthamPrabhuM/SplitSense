{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://splitsense.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://splitsense.onrender.com/auth/:path*"
    }
  ]
}
